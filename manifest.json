{
    "manifest_version": 2,
    "name": "css-inject",
    "description": "Utility to inject css",
    "version": "1.0",
    "icons": {
        "48": "images/icon48.png"
    },
    "browser_action": {
        "default_icon": "images/icon32.png",
        "default_title": "CSS Inject",
        "default_popup": "popup/main.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}